{
    "name": "volodymyr-klymniuk/dto-mapper-bundle",
    "description": "Symfony bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DtoMapperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Volodymyr Klymniuk",
            "email": "Volodymyr.Klymniuk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ocramius/generated-hydrator": "4.6.x-dev",
        "volodymyr-klymniuk/dto-mapper": "dev-master",
        "symfony/proxy-manager-bridge": "6.3.x-dev",
        "symfony/config": "6.3.x-dev",
        "symfony/dependency-injection": "6.3.x-dev",
        "symfony/http-kernel": "6.3.x-dev",
        "doctrine/annotations": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/framework-bundle": "6.3.x-dev",
        "symfony/yaml": "6.3.x-dev"
    },
    "keywords": [
        "deserialization",
        "serializer",
        "object conversion",
        "array conversion",
        "deserialization",
        "performance",
        "hydrator",
        "serialization",
        "symfony",
        "dto-mapper",
        "dto",
        "mapper",
        "php"
    ]
}
